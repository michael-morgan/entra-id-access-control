{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
      "Microsoft.Graph": "Warning",
      "Microsoft.Identity": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FeatureManagement": {
    "GraphApi": false
  },
  "EntraId": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "(from user secrets)",
    "TenantId": "(from user secrets)",
    "ClientId": "(from user secrets)",
    "ClientSecret": "(from user secrets)",
    "CallbackPath": "/signin-oidc",
    "SignedOutCallbackPath": "/signout-callback-oidc"
  },
  "DownstreamApi": {
    "BaseUrl": "https://graph.microsoft.com/v1.0",
    "Scopes": "User.Read User.Read.All Group.Read.All GroupMember.Read.All"
  },
  "GraphApi": {
    "CacheDurationMinutes": 15,
    "DefaultPageSize": 100,
    "MaxBatchSize": 20
  },
  "ConnectionStrings": {
    "AccessControlDb": "(from user secrets)"
  }
}
